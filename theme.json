{
	"workbench.colorTheme": "Theme",
	"files.autoSave": "afterDelay",
	"editor.fontLigatures": true,
	"editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
	"notebook.formatOnSave.enabled": true,
	"editor.suggestFontSize": 22,
	"debug.console.fontSize": 22,
	"markdown.preview.fontSize": 22,
	"notebook.markup.fontSize": 22,
	"chat.editor.fontSize": 22,
	"screencastMode.fontSize": 22,
	"scm.inputFontSize": 22,
	"workbench.iconTheme": "material-icon-theme",
	"tabnine.experimentalAutoImports": true,
	"workbench.productIconTheme": "fluent-icons",
	"editor.cursorBlinking": "expand",
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.fontFamily": "FiraCode Nerd Font",
	"editor.bracketPairColorization.enabled": true,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.formatOnSave": true,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"variable.css",
					"entity.name.function",
					"meta.object-literal.key",
					"keyword.operator.logical",
					"keyword.operator.assignment",
					"support.type.property-name.css",
					"variable.other.object.property",
					"variable.parameter.layer.tailwind",
					"entity.other.attribute-name.class.css",
					"keyword.operator.logical.and.media.css",
					"keyword.operator.logical.only.media.css"
				],
				"settings": {
					"foreground": "#3dd5fb"
				}
			},
			{
				"scope": [
					"variable.parameter",
					"keyword.operator.new",
					"constant.numeric.css",
					"variable.other.object",
					"constant.numeric.json",
					"variable.other.object",
					"storage.modifier.async",
					"constant.language.json",
					"constant.language.null",
					"support.class.component",
					"constant.numeric.decimal",
					"support.function.url.css",
					"support.type.object.module",
					"constant.numeric.integer.shell",
					"variable.other.constant.object",
					"constant.numeric.json.comments",
					"constant.language.boolean.true",
					"constant.language.boolean.false",
					"entity.name.type.instance.jsdoc",
					"entity.other.attribute-name.class.css"
				],
				"settings": {
					"foreground": "#76f5cf"
				}
			},
			{
				"scope": [
					"storage.type",
					"keyword.control.flow",
					"keyword.control.from",
					"keyword.control.export",
					"keyword.control.import",
					"keyword.control.default",
					"keyword.control.trycatch",
					"keyword.operator.ternary",
					"constant.language.undefined",
					"keyword.other.important.css",
					"keyword.control.conditional",
					"storage.type.function.arrow",
					"entity.other.attribute-name",
					"support.constant.font-name.css",
					"markup.underline.link.markdown",
					"variable.other.assignment.shell",
					"entity.other.attribute-name.html",
					"keyword.control.at-rule.media.css",
					"keyword.control.at-rule.import.css",
					"support.constant.property-value.css",
					"keyword.control.at-rule.apply.tailwind",
					"keyword.control.at-rule.layer.tailwind",
					"punctuation.definition.heading.markdown",
					"keyword.control.at-rule.tailwind.tailwind"
				],
				"settings": {
					"foreground": "#e39ff8"
				}
			},
			{
				"scope": [
					"entity.name.tag",
					"entity.name.tag.css",
					"entity.name.tag.html",
					"variable.argument.css",
					"variable.other.constant",
					"meta.object-literal.key",
					"variable.other.property",
					"variable.other.readwrite",
					"keyword.other.unit.s.css",
					"variable.object.property",
					"keyword.other.unit.px.css",
					"support.variable.property",
					"keyword.other.unit.rem.css",
					"entity.name.section.markdown",
					"entity.name.tag.wildcard.css",
					"variable.other.readwrite.alias",
					"support.type.property-name.json",
					"variable.other.constant.property",
					"keyword.other.unit.percentage.css",
					"variable.parameter.tailwind.tailwind",
					"support.variable.property.importmeta",
					"punctuation.definition.metadata.markdown",
					"support.type.property-name.json.comments",
					"entity.other.attribute-name.pseudo-class.css",
					"punctuation.support.type.property-name.end.json",
					"punctuation.support.type.property-name.begin.json",
					"punctuation.support.type.property-name.end.json.comments",
					"punctuation.support.type.property-name.begin.json.comments"
				],
				"settings": {
					"foreground": "#fd919a"
				}
			}
		]
	},
	"workbench.colorCustomizations": {
		"editorBracketHighlight.foreground1": "#61AFEF",
		"editorBracketHighlight.foreground2": "#df93f6",
		"editorBracketHighlight.foreground3": "#76f5cf",
		"tab.activeBorder": "#ffffff",
		"tab.activeForeground": "#ffffff",
		"statusBar.foreground": "#ffffff",
		"sideBarTitle.foreground": "#ffffff",
		"activityBar.foreground": "#ffffff",
		"activityBar.activeBorder": "#008cff",
		"activityBarBadge.foreground": "#ffffff",
		"editorSuggestWidget.focusHighlightForeground": "#008cff"
	},
	"editor.minimap.enabled": false,
	"editor.wordWrap": "on",
	"terminal.integrated.cursorBlinking": true,
	"powermode.enabled": true,
	"powermode.combo.location": "off",
	"powermode.explosions.customCss": {},
	"powermode.shake.intensity": 0,
	"powermode.presets": "flames",
	"workbench.startupEditor": "none",
	"security.workspace.trust.untrustedFiles": "open",
	"auto-close-tag.excludedTags": [
		"area",
		"base",
		"br",
		"col",
		"command",
		"embed",
		"hr",
		"img",
		"input",
		"keygen",
		"link",
		"meta",
		"param",
		"source",
		"track",
		"wbr"
	],
	"editor.mouseWheelZoom": true,
	"editor.fontSize": 22,
	"editor.fontVariations": false,
	"editor.inlayHints.fontSize": 22,
	"editor.linkedEditing": true,
	"css.lint.unknownAtRules": "ignore",
	"tailwindCSS.classAttributes": [
		"class",
		"className",
		"ngClass",
		"activeClassName"
	],
	"powermode.shake.enabled": false,
	"window.zoomLevel": 2,
	"git.autofetch": true,
	"editor.fontWeight": "400",
	"task.slowProviderWarning": true,
	"scss.lint.unknownAtRules": "ignore",
	"scss.lint.unknownProperties": "ignore",
	"less.lint.unknownProperties": "ignore",
	"less.lint.unknownAtRules": "ignore",
	"chat.editor.wordWrap": "on"
}
